{
  "name": "@codingame/monaco-vscode-api-demo",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "author": {
    "name": "CodinGame",
    "url": "http://www.codingame.com"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "node node_modules/vscode/monaco-treemending.js",
    "start": "NODE_OPTIONS=--experimental-import-meta-resolve vite --config vite.config.ts",
    "debug": "vite --config vite.config.ts --debug --force",
    "build": "vite --config vite.config.ts build && rm dist/assets/*.mp3 dist/assets/*.map",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@stadtlandnetz/rollup-plugin-postprocess": "^1.1.0",
    "@types/vscode": "latest",
    "ansi-colors": "latest",
    "monaco-editor": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vscode": "npm:@codingame/monaco-vscode-api",
    "vscode-oniguruma": "latest",
    "xterm": "latest",
    "xterm-addon-canvas": "latest",
    "xterm-addon-image": "latest",
    "xterm-addon-search": "latest",
    "xterm-addon-serialize": "latest",
    "xterm-addon-unicode11": "latest",
    "xterm-addon-webgl": "latest"
  },
  "enabledApiProposals": [
    "fileSearchProvider",
    "textSearchProvider"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
